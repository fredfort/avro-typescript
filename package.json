{
  "name": "create-typescript-from-avro",
  "version": "0.2.0",
  "description": "TypeSript code generator for Apache Avro types",
  "main": "./dist/index.js",
  "bin": {
    "avro-to-ts": "./dist/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build-main": "rollup -c rollup.main.config.js",
    "build-cli": "rollup -c rollup.cli.config.js",
    "build": "npm run build-main && npm run build-cli",
    "test": "mocha -r ts-node/register test/*.spec.ts",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "run-cli": "npm run build && node dist/cli.js gen -e const-enum -f input.avsc > output.ts",
    "diagnose-cli": "npm run build && node dist/cli.js diagnose -f input.avsc",
    "help-cli": "npm run build && node dist/cli.js gen -h"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fredfort/avro-typescript.git"
  },
  "readme": "READMD.md",
  "types": "./dist/index.d.ts",
  "keywords": [
    "avro",
    "typescript"
  ],
  "author": "Fred Fort",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fredfort/avro-typescript/issues"
  },
  "homepage": "https://github.com/fredfort/avro-typescript#readme",
  "devDependencies": {
    "@types/argparse": "^1.0.34",
    "@types/chai": "^4.1.4",
    "@types/chalk": "^2.2.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^7.0.69",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "rollup": "^0.65.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "chalk": "^2.4.1",
    "constant-case": "^2.0.0",
    "prettier": "^1.14.2"
  }
}
